DEFINE x {
    num : 1,
    str: 'a',
    list: [2, 'b'],
    'str1': 'c',
    'part1:part2': 'd',
    "str2": "e",
    ":::": "f"
}

SHOW $x.num
SHOW $x[num]
SHOW $x['num']
SHOW $x["num"]
SHOW $x.str
SHOW $x.list[0]
SHOW $x.list[1]
SHOW $x['list'][0]
SHOW $x['list'][1]
SHOW $x["str1"]
SHOW $x["part1:part2"]
SHOW $x["str2"]
SHOW $x[":::"]
SHOW $x['str1']
SHOW $x['part1:part2']
SHOW $x['str2']
SHOW $x[':::']
